{
  "name": "ampersand-rest-collection",
  "description": "ampersand-collection with REST and Underscore mixins.",
  "version": "2.0.4",
  "author": "Henrik Joreteg <henrik@andyet.net>",
  "browserify": {
    "transform": [
      "ampersand-version"
    ]
  },
  "bugs": {
    "url": "https://github.com/ampersandjs/ampersand-rest-collection/issues"
  },
  "dependencies": {
    "ampersand-collection": "^1.3.0",
    "ampersand-collection-rest-mixin": "^4.0.0",
    "ampersand-collection-underscore-mixin": "^1.0.0",
    "ampersand-version": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^5.11.0",
    "phantomjs": "^1.9.7-15",
    "precommit-hook": "*",
    "run-browser": "^1.3.1",
    "tap-spec": "^0.2.1",
    "tape": "^2.14.0",
    "tape-run": "^0.3.0"
  },
  "homepage": "https://github.com/ampersandjs/ampersand-rest-collection",
  "keywords": [
    "collection",
    "rest",
    "models"
  ],
  "license": "MIT",
  "main": "ampersand-rest-collection.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ampersandjs/ampersand-rest-collection"
  },
  "scripts": {
    "start": "run-browser test/*",
    "test": "browserify test/* | tape-run | tap-spec"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
